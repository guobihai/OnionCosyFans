<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/tv_income_levelname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:text="@string/my_monye_type"
        android:textColor="@color/bluce"
        android:textSize="@dimen/base_font_size_13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/tv_income_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="3dp"
        android:textColor="@color/text"
        android:textSize="@dimen/base_font_size_13"
        android:text=":"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_income_levelname"
        app:layout_constraintLeft_toRightOf="@+id/tv_income_levelname" />

    <TextView
        android:id="@+id/tv_income_level_realFee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="3dp"
        android:text="x1"
        android:textSize="@dimen/base_font_size_13"
        android:textColor="@color/orange"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_income_item"
        app:layout_constraintLeft_toRightOf="@+id/tv_income_item" />

</android.support.constraint.ConstraintLayout>